{
  "$schema": "http://json-schema.org/schema#",
  "$schemaVersion": "0.0",
  "$id": "https://raw.githubusercontent.com/albertoabellagarcia/context/coronavirus/master/schema.jsonld",
  "title": " corona Virus patient data model",
  "description": "An data interchange format for making possible to interchange data about patients in an anonymous way",
  "type": "object",
    "properties":
      {
        "type": {
          "type": "string",
          "enum": ["coronaPatient"]
        },
        "methodDetermineInfection":
        {
          "type": "array",
          "items":
          {
            "type": "string",
            "enum" : ["PCR"],
          }
        },
        "locationInfection" : {
          "$ref": "https://smart-data-models.github.io/data-models/common-schema.json#/definitions/Location-Commons"
        },
        "patientResolution" : {
          "type": "string",
          "enum": ["death", "healing"]
        },
        "locationResolution" : {
          "$ref": "https://smart-data-models.github.io/data-models/common-schema.json#/definitions/Location-Commons"
        },
        "treatments" :
        {
          "type" : "array",
          "items":
          {
            "type": "object",
            "properties" :
            {
              "dateTreatment" :
              {
                "type": "string",
                "format": "date-time"
              },
              "descriptionTreatment" :
              {
                "type": "string"
              },
              "dateEndTreatment" :
              {
                "type": "string",
                "format": "date-time"
              }
            }
          }
        },
        "coMorbidities":
        {
          "type": "array",
          "items":
          {
            "type": "string",
            "enum": ["morbid1", "morbid2"],
          }
        },
       },
  "required": ["@id", "@type"]
}